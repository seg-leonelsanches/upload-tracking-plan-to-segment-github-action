{
    "name": "upload-tracking-plan-to-segment-github-action",
    "description": "A GitHub Action that uploads multiple tracking plans to Segment",
    "version": "0.0.0",
    "author": "Leonel Sanches",
    "private": true,
    "homepage": "https://github.com/seg-leonelsanches/upload-tracking-plan-to-segment-github-action",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/seg-leonelsanches/upload-tracking-plan-to-segment-github-action.git"
    },
    "bugs": {
        "url": "https://github.com/seg-leonelsanches/upload-tracking-plan-to-segment-github-action/issues"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.7",
        "@typescript-eslint/eslint-plugin": "^7.6.0",
        "@typescript-eslint/parser": "^7.6.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "@actions/core": "^1.10.1",
        "axios": "^1.6.8"
    },
    "jest": {
        "preset": "ts-jest",
        "verbose": true,
        "clearMocks": true,
        "testEnvironment": "node",
        "moduleFileExtensions": [
            "js",
            "ts"
        ],
        "testMatch": [
            "**/*.test.ts"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/dist/"
        ],
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "coverageReporters": [
            "json-summary",
            "text",
            "lcov"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "./src/**"
        ]
    }
}
